{
  "version": 1,
  "projects": {
    "deprecation-crawler": {
      "root": "packages/deprecation-crawler",
      "sourceRoot": "packages/deprecation-crawler/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/deprecation-crawler/tsconfig.lib.json",
              "packages/deprecation-crawler/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/deprecation-crawler/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/deprecation-crawler/jest.config.js",
            "tsConfig": "packages/deprecation-crawler/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/deprecation-crawler",
            "tsConfig": "packages/deprecation-crawler/tsconfig.lib.json",
            "packageJson": "packages/deprecation-crawler/package.json",
            "main": "packages/deprecation-crawler/src/index.ts",
            "assets": [
              "packages/deprecation-crawler/*.md",
              "packages/deprecation-crawler/src/cli.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "deprecation-crawler"
}
